<Window x:Class="CSV_Example.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CSV_Example"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btn__ValidateFileExist" Content="Validate" HorizontalAlignment="Left" Margin="414,65,0,0" VerticalAlignment="Top" Click="btn__ValidateFileExist_Click"/>
        <Label x:Name="lbl_PickFile" Content="PickFile" HorizontalAlignment="Left" Margin="10,54,0,0" VerticalAlignment="Top" Width="101"/>
        <TextBox x:Name="txbx_FilePath" HorizontalAlignment="Left" Margin="133,62,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="30"/>
        <Button x:Name="btn_ProcessFile" Content="Process File" Margin="80,97,0,0" Height="39" VerticalAlignment="Top" HorizontalAlignment="Left" Width="226" IsEnabled="False" Click="btn_ProcessFile_Click"/>
        <ListBox x:Name="lbx_Lines" Height="200" Width="750" Margin="10,224,10,10"/>

    </Grid>
</Window>
